@model ck_project.Models.Search
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
@using (Html.BeginForm("Index","Log",FormMethod.Post))
{
    <div class="form-group">
        <div class="col-md-10">
            @Html.EditorFor(Model => Model.start)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            @Html.EditorFor(Model => Model.end)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            @Html.DropDownListFor(Model => Model.Emp, (List<SelectListItem>)Model.Emp)
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Search now" class="btn btn-default" />
        </div>
    </div>



}